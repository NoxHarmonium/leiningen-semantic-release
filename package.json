{
  "name": "leiningen-semantic-release",
  "version": "1.0.0",
  "description": "Automated release management for leiningen projects",
  "main": "dist/index.js",
  "author": "Sean Dawson <contact@seandawson.info>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prepare": "tsc",
    "semantic-release": "semantic-release",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lint-fix": "tslint --fix -p tsconfig.json -c tslint.json",
    "format-check": "prettier-check src/**/*",
    "type-coverage": "type-coverage --strict --detail --ignore-catch",
    "format": "prettier --write '{src,test}/**/*.{ts,tsx}'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/noxharmonium/leiningen-semantic-release.git"
  },
  "dependencies": {
    "@semantic-release/error": "^2.2.0",
    "execa": "^2.0.4",
    "fp-ts": "^2.0.5",
    "io-ts": "^2.0.1",
    "io-ts-types": "^0.5.1",
    "monocle-ts": "^2.0.0",
    "semver": "^6.3.0"
  },
  "devDependencies": {
    "@types/semantic-release": "^15.13.0",
    "@types/semver": "^6.0.2",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "3.0.2",
    "prettier": "^1.18.2",
    "prettier-check": "^2.0.0",
    "semantic-release": "^15.13.24",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-immutable": "^6.0.1",
    "tslint-sonarts": "^1.9.0",
    "type-coverage": "^2.3.0",
    "typescript": "^3.6.3"
  },
  "peerDependencies": {
    "semantic-release": "^15.13.24"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
